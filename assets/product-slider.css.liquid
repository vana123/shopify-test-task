.slider {}

.slider-for {}
.slick-track {}
.slider-nav .slick-list {
  margin: 0 30px;
}
.slider-nav {
  height: 120px;
  display: flex;
  position: relative;
}
.slider-nav .slick-arrow {
  position: absolute;
  top: 50%;
  z-index: 10;
  font-size: 0;
  width: 11px;
  height: 19px;
  background-position: center;
  background-color: #00000000;
  background-repeat: no-repeat;
  border: none;
}
.slider-nav .slick-prev {
  left: 0;
  background-image: url('{{'arrow-left.svg' | asset_url}}');

}
.slider-nav .slick-next {
  right: 0;
  background-image: url('{{'arrow-rght.svg' | asset_url}}');
}
.slider-nav img {
  margin: 0 10px;
}